{
    "compilerOptions": {
        "baseUrl": "./src",
        "target": "es2015",
        "module": "es2015",
        "moduleResolution": "node",
        "jsx": "react",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "dom",
            "es2015",
            "es2016",
            "es2017"
        ]
    }
}

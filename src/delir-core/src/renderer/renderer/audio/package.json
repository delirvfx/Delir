{
    "name": "delir-plugin-audio",
    "version": "0.0.0-alpha.2",
    "author": "Ragg <ragg.devpr@gmail.com>",
    "main": "audio.js",
    "engines": {
        "delir": "0.0.x"
    },
    "delir": {
        "feature": "CustomLayer",
        "targetApi": {
            "renderer": "0.0.x"
        },
        "acceptFileTypes": {
            "wav": "source",
            "webm": "source",
            "mpeg": "source",
            "mp3": "source",
            "ogg": "source"
        }
    }
}
